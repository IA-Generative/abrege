{
  "name": "frontend",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "clean:dist": "rimraf dist",
    "build-only": "vite build",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "cd .. && husky apps/client/.husky",
    "preview": "vite preview",
    "test": "run-s test:unit test:e2e",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:e2e-ci": "playwright test",
    "test:e2e-ui": "playwright test --ui",
    "type-check": "vue-tsc --build --force || exit 0",
    "generate-openapi": "pnpx openapi-typescript ${VITE_ABREGE_API_URL}/openapi.json -o ./src/api/types/api.schema.d.ts"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.14.2",
    "@gouvminint/vue-dsfr": "^8.8.1",
    "@tailwindcss/vite": "^4.1.16",
    "axios": "^1.12.2",
    "dompurify": "^3.3.0",
    "focus-trap": "^7.6.5",
    "focus-trap-vue": "^4.1.0",
    "husky": "^9.1.7",
    "keycloak-js": "^26.2.1",
    "marked": "^16.4.1",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.16",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.6.3",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@playwright/test": "^1.56.1",
    "@rushstack/eslint-patch": "^1.14.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.6",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-plugin-playwright": "^2.2.2",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.10.1",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.2.0",
    "vite": "^7.1.12",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1",
    "workbox-window": "^7.3.0"
  }
}
