    "dompurify": "^3.2.6",
    "focus-trap": "^7.6.5",
    "focus-trap-vue": "^4.1.0",
    "husky": "^9.1.7",
    "keycloak-js": "^26.2.0",
    "marked": "^15.0.12",
    "pinia": "^2.3.1",
    "tailwindcss": "^4.1.13",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.21",
    "vue-matomo": "^4.2.0",
